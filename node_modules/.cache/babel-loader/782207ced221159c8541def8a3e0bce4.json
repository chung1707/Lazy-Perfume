n      }\n      this.filterSelected.seasonId= id;\n    },\n    selectRangePrice(id) {\n      if (this.filterSelected.priceRangeId == id) {\n        return (this.filterSelected.priceRangeId = null);\n      }\n      this.filterSelected.priceRangeId= id;\n    },\n    setArrangKey(key){\n      this.arrangeKey = key.name;\n      this.arrangeActive = false;\n    }\n  },\n};\n</script>\n\n<style></style>\n"]}]}